<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.hit.model.Activity" table="ACTIVITY">
        <id name="activityId" type="int">
            <column name="ACTIVITY_ID" />
            <generator class="increment"></generator>
        </id>
        <property name="userName" type="java.lang.String">
            <column name="USER_NAME" />
        </property>
        <property name="activityName" type="java.lang.String">
            <column name="ACTIVITY_NAME" />
        </property>
         <property name="trainingDay" type="java.lang.String">
            <column name="TRAINING_DAY" />
        </property>
        <property name="numberOfSets" type="int">
            <column name="NUMBERS_OF_SETS" />
        </property>
        <property name="numberOfReps" type="int">
            <column name="NUMBERS_OF_REPS" />
        </property>
    </class>
</hibernate-mapping>
